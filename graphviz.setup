(include "setup-helper")

(verify-extension-name "graphviz")

(setup-shared-extension-module
 'graphviz
 (extension-version "0.8")
 compile-options: '(-X cock))

;;; Generate docs.
(handle-exceptions exn
  (warning "Not generating documentation")
  (include "setup-helper-cock")
  (run-cock -o graphviz.wiki graphviz-core.scm))
